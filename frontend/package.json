{
  "name": "futureknx",
  "version": "1.0.16",
  "private": true,
  "scripts": {
    "prebuild": "rimraf .output",
    "build": "nuxt build",
    "postbuild": "cd .output/public/_nuxt && magick mogrify -path ./ -strip -format jpg -quality 80 -resize \">1920\" *.jpg && magick mogrify -path ./ -strip -quality 50 -resize \">900\" *.png && magick mogrify -path ./ -strip -quality 50 -resize \">1920\" -define webp:lossless=true *.webp",
    "dev": "nuxt dev --port=5678 --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "predeploy": "cross-env NITRO_PRESET=firebase npm run build",
    "deploy": "npx firebase-tools deploy",
    "postdeploy": "git add --all && git commit -am \"Deploy %npm_package_version%\" && npm version patch && git push",
    "serve": "npx firebase-tools serve --only hosting"
  },
  "devDependencies": {
    "@averjs/nuxt-compression": "^1.2.2",
    "@dargmuesli/nuxt-cookie-control": "^7.4.0",
    "@indielayer/ui": "^1.7.1",
    "@nuxtjs/tailwindcss": "^6.11.3",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "autoprefixer": "^10.4.17",
    "compress-images": "^1.9.9",
    "cross-env": "^7.0.3",
    "floating-vue": "^5.2.2",
    "gzip-cli": "^1.2.0",
    "nuxt": "^3.10.1",
    "postcss": "^8.4.35",
    "ramda": "^0.28.0",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "sass-loader": "^13.3.3",
    "sitemap": "^7.1.1",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.0"
  },
  "dependencies": {
    "@ckpack/vue-color": "^1.5.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@google/model-viewer": "^3.4.0",
    "@types/three": "^0.160.0",
    "@vuepic/vue-datepicker": "^8.5.0",
    "animate.css": "^4.1.1",
    "dom-to-pdf": "^0.3.2",
    "drag-drop-touch": "^1.3.1",
    "flipbook-vue": "^1.0.0-alpha.4",
    "html2canvas": "^1.4.1",
    "lax.js": "^2.0.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "three": "^0.160.1",
    "vite-imagetools": "^6.2.9",
    "vue-gtag-next": "^1.14.0",
    "vue3-observe-visibility": "^0.1.3",
    "vue3-parallax": "^0.1.4",
    "weather-icons": "github:basmilius/weather-icons#master",
    "web-vitals": "^3.5.2"
  }
}
